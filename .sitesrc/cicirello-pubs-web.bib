author = {Demetrios Roubos and Shawn Casler and James Hedigan and Ryan Shaw and Suvineetha Herath and David Costantino and Vincent Cicirello and Robert Kachur and Ajantha Herath},
title = {A Study of Wireless Security Privacy and Forensics},
