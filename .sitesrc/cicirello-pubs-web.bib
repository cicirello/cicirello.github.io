@INPROCEEDINGS{FLAIRS2010,
author = {Vincent A. Cicirello},
title = {Heuristic Sequencing Crossover: Integrating Problem Dependent 
         Heuristic Knowledge into a Genetic Algorithm},
booktitle = {Proceedings of the Twenty-Third International 
             Florida Artificial Intelligence Research Society Conference, FLAIRS-23},
publisher = {AAAI Press},
month = {May},
