@INPROCEEDINGS{GECCO2006,
author = {Vincent A. Cicirello},
title = {Non-Wrapping Order Crossover: An Order Preserving
Crossover Operator that Respects Absolute Position},
booktitle = {Proceedings of the Genetic and Evolutionary Computation Conference (GECCO'06)},
year = {2006},
month = {July},
pages = {1125--1131},
