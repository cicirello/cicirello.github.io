@INPROCEEDINGS{BICT2014,
author = {Vincent A. Cicirello},
title = {On the Effects of Window-Limits on the Distance Profiles of Permutation Neighborhood Operators},
booktitle = {Proceedings of the 8th International Conference on Bio-inspired Information and Communications Technologies},
