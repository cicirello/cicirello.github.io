@INPROCEEDINGS{FAABS2004,
AUTHOR = {Maxim D. Peysakhov and Vincent A. Cicirello and William C. Regli},
TITLE = {Ecology Based Decentralized Agent Management System},
BOOKTITLE = {Formal Approaches to Agent-Based Systems: 
Third International Workshop, FAABS 2004, Revised Selected Papers},
YEAR = {2004},
MONTH = {April},
PUBLISHER = {Springer-Verlag},
SERIES = {Lecture Notes in Computer Science},
volume = {LNCS 3228},
PAGES = {1--11},
pdf = {FAABS2004.pdf},
acro = {faabs},
titletag = {Formal Approaches to Agent-Based Systems},
doi = {http://dx.doi.org/10.1007/978-3-540-30960-4_1},
abstract = {The problem of maintaining a desired number of mobile agents on a network is not trivial, 
especially if we want a completely decentralized solution. Decentralized control makes a system more 
robust and less susceptible to partial failures. The problem is exacerbated on wireless ad hoc networks 
where host mobility can result in significant changes in the network size and topology. In this paper 
we propose an ecology-inspired approach to the management of the number of agents. The approach associates 
agents with living organisms and tasks with food. Agents procreate or die based on the abundance of 
uncompleted tasks (food). We performed a series of experiments investigating properties of such systems and 
analyzed their stability under various conditions. We concluded that the ecology based metaphor can be 
successfully applied to the management of agent populations on wireless ad hoc networks.}
}
